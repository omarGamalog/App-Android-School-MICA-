<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/cardview_dark_background">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:id="@+id/ImageNameTextView"
            android:layout_width="407dp"
            android:layout_height="109dp"
            android:background="@color/cardview_light_background"
            android:gravity="center"
            android:text="JSon Image Name"
            android:textColor="#000"
            android:textSize="20dp"
            card_view:layout_constraintBottom_toBottomOf="parent"
            card_view:layout_constraintEnd_toEndOf="parent"
            card_view:layout_constraintStart_toStartOf="parent"
            card_view:layout_constraintTop_toTopOf="parent"
            card_view:layout_constraintVertical_bias="0.914" />

        <ImageView
            android:id="@+id/Image111"
            android:layout_width="359dp"
            android:layout_height="470dp"
            android:background="@color/cardview_dark_background"
            card_view:layout_constraintBottom_toTopOf="@+id/ImageNameTextView"
            card_view:layout_constraintEnd_toEndOf="parent"
            card_view:layout_constraintStart_toStartOf="parent"
            card_view:layout_constraintTop_toTopOf="parent"
            card_view:layout_constraintVertical_bias="0.04"
            card_view:srcCompat="@drawable/common_google_signin_btn_icon_light_normal_background"
            android:contentDescription="TODO" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.cardview.widget.CardView>